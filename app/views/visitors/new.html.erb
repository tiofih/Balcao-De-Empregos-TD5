<% @visitor.errors&.full_messages&.each do |m| %>
  <%= m %>
<% end %>
<%= form_with model: @visitor do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :cpf %>
    <%= f.text_field :cpf %>
  </div>
  <div>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
  </div>
  <div>
    <%= f.label :github %>
    <%= f.text_field :github %>
  </div>
  <div>
    <%= f.submit 'Finalizar Cadastro' %>
  </div>
<% end %>
